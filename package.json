{
  "name": "duet-ecommerce",
  "version": "1.0.0",
  "description": "Duet e-commerce system with separated frontend and backend",
  "main": "backend/server.js",
  "scripts": {
    "start": "cd backend && npm start",
    "dev": "cd backend && npm run dev",
    "dev:env": "cd backend && npm run dev:env",
    "prod:env": "cd backend && npm run prod:env",
    "build": "node scripts/build.js",
    "install:backend": "cd backend && npm install",
    "install:all": "npm install && npm run install:backend"
  },
  "keywords": ["e-commerce", "frontend", "backend", "mongodb", "express"],
  "author": "Duet Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "fs-extra": "^11.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
